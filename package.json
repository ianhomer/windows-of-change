{
  "name": "windows-of-change",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "eslint": "eslint src --ext .ts,.tsx",
    "eslint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint": "run-s prettier eslint",
    "lint:fix": "run-s prettier:fix eslint:fix",
    "prettier": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "update": "yarn run npm-check-updates -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --quiet",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,tsx,yaml}": "prettier --write"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@types/d3": "^6.3.0",
    "@types/styled-components": "^5.1.9",
    "d3": "^6.7.0",
    "lodash": "^4.17.21",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "17.0.2",
    "remark-gfm": "^1.0.0",
    "sass": "^1.32.13",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.169",
    "@types/node": "^15.0.3",
    "@types/react": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "npm-check-updates": "^11.5.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "react-markdown": "^6.0.2",
    "typescript": "^4.2.4"
  }
}
