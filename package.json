{
  "name": "windows-of-change",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "eslint": "yarn run eslint src --ext .ts,.tsx",
    "lint": "eslint",
    "prettier": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "start": "next start",
    "test": "jest",
    "update": "yarn run npm-check-updates -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --quiet",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,tsx,yaml}": "prettier --write"
  },
  "dependencies": {
    "next": "10.0.3",
    "npm-check-updates": "^10.2.5",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDepencies": {
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.2",
    "enzyme": "^3.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "typescript": "^4.1.3"
  }
}
